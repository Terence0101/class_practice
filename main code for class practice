class student:
  def __init__ (self,name,gender):
    self.name = name
    self.gender = gender
    self.grades = []
  def add (self,grade):
    self.grades.append (grade)
  def avg (self):
    avg_grade = sum(self.grades) / len(self.grades)
    return avg_grade
  def fcount (self):
    fail_grade = 0
    for i in self.grades:
      if i < 60:
        fail_grade += 1
    return fail_grade

def top (*students):
  top_student = sorted (list(students), key=lambda, student=student.avg(), reverse=True)
  return top_student[0]
      
